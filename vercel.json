{
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "https://www.ignoudistance.in/$1",
      "permanent": true,
      "has": [
        {
          "type": "host",
          "value": "mba.ignoudistance.in"
        }
      ]
    }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/" }
  ]
}
